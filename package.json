{
  "name": "sapper-tailwind-typescript-netlify-functions-starter",
  "description": "A starter for sapper, tailwind, typescript, and netlify functions",
  "version": "0.0.1",
  "scripts": {
    "watch:css": "postcss src/assets/global.pcss -o static/global.css -w",
    "watch:dev": "sapper dev",
    "dev": "cross-env NODE_ENV=dev run-p watch:* dev:server",
    "dev:server": "netlify-lambda serve functions",
    "build": "run-s build:css build:export build:lambda",
    "build:css": "NODE_ENV=production postcss src/assets/global.pcss -o static/global.css",
    "build:sapper": "sapper build",
    "build:export": "sapper export",
    "build:lambda": "netlify-lambda build functions",
    "export": "run-s build:css build:export",
    "start": "node __sapper__/build",
    "serve": "serve ___sapper__/export"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "netlify-lambda": "^2.0.2",
    "polka": "next",
    "sirv": "^1.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.12.5",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.1.0",
    "@rollup/plugin-url": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.14.19",
    "@types/polka": "^0.5.1",
    "cssnano": "^4.1.10",
    "http-proxy-middleware": "^1.0.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.2",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.35.1",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "svelte": "^3.31.1",
    "svelte-check": "^1.1.24",
    "svelte-preprocess": "^4.6.1",
    "tailwindcss": "^2.0.2",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  }
}
